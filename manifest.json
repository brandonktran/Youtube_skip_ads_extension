{
  "manifest_version": 2,
  "name": "Skip Youtube Ads",
  "description": "Removes Youtube promos & banners, and skips ads.",
  "version": "1.2",
  "icons": {
    "128": "icon128.png"
  },

  "browser_action": {
    "default_icon": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "skip.js"
      ]
    }
  ]
}
